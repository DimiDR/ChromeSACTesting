{
  "manifest_version": 3,
  "name": "Button Clicker",
  "version": "1.0",
  "description": "Clicks a button with ID '12345'",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": ["activeTab", "nativeMessaging", "tabs"],
  "background": {
    "scripts": ["background.js", "popup.js"],
    "persistent": false
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
